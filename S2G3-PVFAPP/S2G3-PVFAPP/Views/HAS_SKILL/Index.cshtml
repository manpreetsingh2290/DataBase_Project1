@model IEnumerable<S2G3_PVFAPP.Models.HAS_SKILL>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Enable_Flag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMPLOYEE.Employee_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SKILL.Skill_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Enable_Flag)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMPLOYEE.Employee_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SKILL.Skill_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { employeeID=item.Employee_ID, skillID=item.Skill_ID }) |
            @Html.ActionLink("Details", "Details", new { employeeID=item.Employee_ID, skillID=item.Skill_ID }) |
            @Html.ActionLink("Delete", "Delete", new { employeeID=item.Employee_ID, skillID=item.Skill_ID })
        </td>
    </tr>
}

</table>
